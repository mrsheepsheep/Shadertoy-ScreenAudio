{
    "manifest_version": 2,
    "name": "Shadertoy Screen Audio",
    "description": "Override Microphone with Screen Audio in Shadertoy",
    "version": "1.0",
    "content_scripts": [
        {
            "matches": ["https://www.shadertoy.com/view/*"],
            "all_frames": false,
            "js": ["inject.js"]
        }
    ],
    "web_accessible_resources": ["effect.js"]
}